
@{
    ViewBag.Title = "Report DCFC";
}

<style>
    .container {
        position: absolute;
        width: 100%;
        overflow: hidden;
        padding-top: 56.25%; /* 16:9 Aspect Ratio */
    }

    .responsive-iframe {
        position: absolute;
        top: 70px;
        left: 0;
        bottom: 0;
        right: 0;
        width: 100%;
        height: 100%;
        border: none;
    }
</style>

<h2>Report Digital Check Fatigue Chamber</h2>

<main id="main-container">
    <div class="content">
        <iframe class="responsive-iframe" src="https://bi.kppmining.com/kpp_bi/report/SHE/Report_Chamber?rs:embed=true"></iframe>
    </div>
</main>

<script>
    $(document).ready(function () {
        $('.responsive-iframe').on('load', function () {
            $('html, body').animate({ scrollTop: 0 }, 'slow');
        });
    });
</script>

