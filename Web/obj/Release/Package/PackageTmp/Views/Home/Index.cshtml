@{
    ViewBag.Title = "Home Page";
    ViewBag.Navlink = "homeindex";
}

<!-- Data Karyawan Fit -->
<h4>Data Karyawan Fit</h4>
<div class="row">
    <!-- Row #1 -->
    <div class="col-md-6 col-xl-4">
        <a class="block block-link-pop" href="javascript:void(0)">
            <div class="block-content block-content-full d-flex align-items-center justify-content-between">
                <div class="p-3">
                    <i class="fa fa-2x fa-user-group text-secondary"></i>
                </div>
                <div class="ms-1 text-start">
                    <p class="fs-sm text-uppercase fw-semibold text-muted mb-0">
                        Total Karyawan Masuk
                    </p>
                    <p class="fs-lg fw-semibold mb-0">
                        @Session["KaryawanMasuk"].ToString()
                    </p>
                    <p class="fw-normal mb-0" style="font-size:small">
                        Update Terakhir @Session["UpdateTerakhirMasuk"].ToString()
                    </p>
                </div>
            </div>
        </a>
    </div>
    <div class="col-md-6 col-xl-4">
        <a class="block block-link-pop" href="javascript:void(0)">
            <div class="block-content block-content-full d-flex align-items-center justify-content-between">
                <div class="p-3">
                    <i class="fa fa-2x fa-user-group text-info"></i>
                </div>
                <div class="ms-1 text-start">
                    <p class="fs-sm text-uppercase fw-semibold text-muted mb-0">
                        Total Karyawan Fit
                    </p>
                    <p class="fs-lg fw-semibold mb-0">
                        @Session["KaryawanFit"].ToString()
                    </p>
                    <p class="fw-normal mb-0" style="font-size:small">
                        Update Terakhir @Session["UpdateTerakhirFit"].ToString()
                    </p>
                </div>
            </div>
        </a>
    </div>
    <div class="col-xl-4">
        <a class="block block-link-pop" href="javascript:void(0)">
            <div class="block-content block-content-full d-flex align-items-center justify-content-between">
                <div class="p-3">
                    <i class="fa fa-2x fa-user-group text-danger"></i>
                </div>
                <div class="ms-1 text-start">
                    <p class="fs-sm text-uppercase fw-semibold text-muted mb-0">
                        Total Karyawan Unfit
                    </p>
                    <p class="fs-lg fw-semibold mb-0">
                        @Session["KaryawanUnfit"].ToString()
                    </p>
                    <p class="fw-normal mb-0" style="font-size:small">
                        Update Terakhir @Session["UpdateTerakhirUnfit"].ToString()
                    </p>
                </div>
            </div>
        </a>
    </div>
    <!-- END Row #1 -->
</div>

<!-- Progress Fatigue Check -->
